{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "tsx",
			"type": "node",
			"request": "launch",
			"program": "main.ts",
			"runtimeExecutable": "tsx",
			"runtimeArgs": ["--env-file=.env"],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"skipFiles": ["<node_internals>/**", "${workspaceFolder}/node_modules/**"]
		}
	]
}
