{
	"categories": {
		"correctness": "error",
		"nursery": "warn",
		"pedantic": "warn",
		"suspicious": "warn",
		"perf": "warn",
		"style": "warn"
	},
	"rules": {
		"no-null": "off",
		"id-length": ["error", { "exceptions": ["z"] }],
		"sort-imports": "off",
		"sort-keys": "off",
		"no-magic-numbers": "off",
		"no-map-spread": "off"
	},
	"env": {
		"node": true
	}
}
